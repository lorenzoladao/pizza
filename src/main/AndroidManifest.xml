<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.project">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Project">
        <activity
            android:name=".StoreOrderActivity"
            android:label="@string/storeOrder"
            android:parentActivityName=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".CurrentActivity"
            android:label="@string/currentOrder"
            android:parentActivityName=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".HawaiianActivity"
            android:label="@string/hawaiianTitle"
            android:parentActivityName=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".DeluxeActivity"
            android:label="@string/deluxe_pizza"
            android:parentActivityName=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".PizzaActivity"
            android:label="@string/pepperoniTitle"
            android:parentActivityName=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>